{{#freestyle-collection defaultKey="no-data" as |collection|}}
  {{#collection.variant
    key='nice-data'
  }}
    {{#freestyle-usage 'KeenChart-niceData' title='Installs Chart - Nice Data' usageTitle='Usage'}}
      {{lsgdd/keen-chart
        class='r-YappAnalytics-chart'
        dataSource=deviceInstallsFirstTime
        chartType='area'
        header='Device Installs'
      }}
    {{/freestyle-usage}}
  {{/collection.variant}}
  {{#collection.variant
    key='no-data'
  }}
    {{#freestyle-usage 'KeenChart-noData' title='Installs Chart - No Data' usageTitle='Usage'}}
      {{lsgdd/keen-chart
        class='r-YappAnalytics-chart'
        dataSource=deviceInstallsFirstTimeNoData
        chartType='area'
        header='Device Installs - No Data'
      }}
    {{/freestyle-usage}}
  {{/collection.variant}}
  {{#collection.variant
    key='waiting'
  }}
    {{#freestyle-usage 'KeenChart-waiting' title='Installs Chart - Waiting' usageTitle='Usage'}}
      {{lsgdd/keen-chart
        class='r-YappAnalytics-chart'
        dataSource=waitingDataSource
        chartType='area'
        header='Device Installs - Waiting'
      }}
    {{/freestyle-usage}}
  {{/collection.variant}}
{{/freestyle-collection}}
